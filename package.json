{
  "name": "eslint-plugin-filename-consistency",
  "version": "1.0.2",
  "engines": {
    "node": ">=14"
  },
  "description": "Eslint rule for consistent file and folder names",
  "license": "MIT",
  "author": {
    "name": "wmdanor"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wmdanor/eslint-plugin-filename-consistency.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "lint",
    "style",
    "consistency",
    "filenames",
    "filename",
    "folders",
    "folder",
    "path"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "typecheck": "tsc",
    "format": "prettier -c **/*.js",
    "format:fix": "prettier -w **/*.js",
    "test": "node --test"
  },
  "devDependencies": {
    "@types/eslint": "^8.8.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.10",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "eslint": ">=8.8.0"
  }
}
