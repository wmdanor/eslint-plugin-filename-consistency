{
  "name": "eslint-plugin-filename-consistency",
  "version": "2.0.0",
  "description": "Eslint rule for consistent file and folder names",
  "license": "MIT",
  "author": "wmdanor",
  "repository": "https://github.com/wmdanor-universe/eslint-plugin-filename-consistency",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "linter",
    "lint",
    "style",
    "consistency",
    "filenames",
    "filename",
    "folders",
    "folder",
    "path"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "pnpm compile",
    "test": "jest",
    "compile": "swc src -d dist --delete-dir-on-start",
    "compile:watch": "pnpm run compile --watch",
    "typecheck": "tsc",
    "typecheck:watch": "pnpm run typecheck --watch --incremental",
    "lint": "eslint src/**/* --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier '**/*.{ts,json,md}'",
    "prettier:fix": "pnpm prettier --write",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.50",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "eslint": ">=8.8.0"
  }
}
